<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="600.0" prefWidth="400.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="application.MainController">
   <children>
      <Label layoutX="85.0" layoutY="64.0" text="BMI Calculator">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <VBox layoutX="84.0" layoutY="224.0" prefHeight="58.0" prefWidth="233.0">
         <children>
            <HBox prefHeight="28.0" prefWidth="219.0">
               <children>
                  <Label alignment="CENTER" prefHeight="17.0" prefWidth="52.0" text="身長">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Label>
                  <TextField fx:id="textFeild_height" />
                  <Label alignment="CENTER" prefHeight="25.0" prefWidth="38.0" text="cm">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
            <HBox prefHeight="28.0" prefWidth="219.0">
               <children>
                  <Label alignment="CENTER" prefHeight="17.0" prefWidth="52.0" text="体重">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Label>
                  <TextField fx:id="textFeild_weight" />
                  <Label alignment="CENTER" prefHeight="25.0" prefWidth="38.0" text="㎏">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
         </children>
      </VBox>
      <HBox layoutX="101.0" layoutY="337.0" prefHeight="45.0" prefWidth="200.0">
         <children>
            <Label prefHeight="46.0" prefWidth="66.0" text="BMI">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <Label fx:id="label_BMI" prefHeight="69.0" prefWidth="134.0" />
         </children>
      </HBox>
      <Button fx:id="button_Enter" layoutX="154.0" layoutY="410.0" mnemonicParsing="false" onAction="#onClickedEnter" prefHeight="28.0" prefWidth="95.0" text="Enter" />
      <Label fx:id="label_message" layoutX="54.0" layoutY="484.0" prefHeight="45.0" prefWidth="293.0">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
